ПутьКСценариюПараметров = ОбъединитьПути(ТекущийСценарий().Каталог, "src", "Модули", "ПараметрыСистемы.os");
ПараметрыПриложения = ЗагрузитьСценарий(ПутьКСценариюПараметров);

Описание.Имя(ПараметрыПриложения.ИмяСистемы())
        .Версия(ПараметрыПриложения.Версия())
        .Автор("Ступников Владимир")
        .АдресАвтора("Stupnikov.vg@gmail.com")
        .Описание("Библиотека для отправки сборки релизов 1С и gitlab")
        .ВерсияСреды("1.0.11")
        .ВключитьФайл("src")
        .ВключитьФайл("tests")
        .ВключитьФайл("packagedef")
        .ЗависитОт("fs",                "1.0.0")
        .ЗависитОт("logos",             "1.3.0")
        .ЗависитОт("1commands",         "1.5.0")
        .ЗависитОт("cmdline",           "1.0.0")
        .ЗависитОт("1testrunner",       "1.7.0")
        .ЗависитОт("tempfiles",         "0.2.2")
        .ИсполняемыйФайл("src/main.os");
        ;